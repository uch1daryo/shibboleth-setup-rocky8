  <Organization>
    <OrganizationName xml:lang="en">{{ idp_organization_name_en }}</OrganizationName>
    <OrganizationName xml:lang="ja">{{ idp_organization_name_ja }}</OrganizationName>
    <OrganizationDisplayName xml:lang="en">{{ idp_organization_display_name_en }}</OrganizationDisplayName>
    <OrganizationDisplayName xml:lang="ja">{{ idp_organization_display_name_ja }}</OrganizationDisplayName>
    <OrganizationURL xml:lang="en">{{ idp_organization_url }}</OrganizationURL>
  </Organization>
  <ContactPerson contactType="technical">
    <GivenName>{{ idp_contact_person_given_name }}</GivenName>
    <SurName>{{ idp_contact_person_sur_name }}</SurName>
    <EmailAddress>mailto:{{ idp_contact_person_email_address }}</EmailAddress>
  </ContactPerson>
</EntityDescriptor>
