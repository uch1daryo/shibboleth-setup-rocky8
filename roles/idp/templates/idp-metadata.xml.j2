<!-- IdP: Your Organization Name, Your IdP Site URL, Date -->
<EntityDescriptor xmlns="urn:oasis:names:tc:SAML:2.0:metadata" entityID="{{ idp_entity_id }}">
  <IDPSSODescriptor protocolSupportEnumeration="urn:oasis:names:tc:SAML:1.1:protocol urn:mace:shibboleth:1.0 urn:oasis:names:tc:SAML:2.0:protocol">
    <Extensions>
      <shibmd:Scope xmlns:shibmd="urn:mace:shibboleth:metadata:1.0" regexp="false">{{ idp_scope }}</shibmd:Scope>
      <mdui:UIInfo xmlns:mdui="urn:oasis:names:tc:SAML:metadata:ui">
        <mdui:DisplayName xml:lang="ja">{{ idp_display_name_ja }}</mdui:DisplayName>
        <mdui:DisplayName xml:lang="en">{{ idp_display_name_en }}</mdui:DisplayName>
        <mdui:Logo height="50" width="50">{{ idp_logo_url }}</mdui:Logo>
        <mdui:InformationURL xml:lang="ja">{{ idp_info_url_ja }}</mdui:InformationURL>
        <mdui:InformationURL xml:lang="en">{{ idp_info_url_en }}</mdui:InformationURL>
        <mdui:PrivacyStatementURL xml:lang="ja">{{ idp_privacy_url_ja }}</mdui:PrivacyStatementURL>
        <mdui:PrivacyStatementURL xml:lang="en">{{ idp_privacy_url_en }}</mdui:PrivacyStatementURL>
        <mdui:Keywords xml:lang="en">category:location:others category:organizationType:others</mdui:Keywords>
      </mdui:UIInfo>
      <mdui:DiscoHints xmlns:mdui="urn:oasis:names:tc:SAML:metadata:ui">
        <mdui:IPHint>{{ idp_ip_hint }}</mdui:IPHint>
        <mdui:DomainHint>{{ idp_domain_hint }}</mdui:DomainHint>
        <mdui:GeolocationHint>geo:35.6924668,139.75810019999994</mdui:GeolocationHint>
      </mdui:DiscoHints>
    </Extensions>
